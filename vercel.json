{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        },
        {
            "src": "/static/css/(.*).css",
            "dest": "/static/css/$1.css?v=1234"
        }
    ]
}